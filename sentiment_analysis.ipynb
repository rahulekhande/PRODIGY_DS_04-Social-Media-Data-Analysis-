#Cell 1 – Import Libraries
import pandas as pd
import matplotlib.pyplot as plt

#Cell 2 – Load Dataset
data = pd.read_csv("social_media_data.csv")
data.head()

#Cell 3 – Dataset Info
data.info()
data.isnull().sum()


#Cell 4 – Platform Distribution
data['platform'].value_counts().plot(kind='bar')

plt.title("Platform Distribution")
plt.xlabel("Platform")
plt.ylabel("Count")
plt.show()


#Cell 5 – Engagement Analysis (Likes)
data.groupby('platform')['likes'].mean().plot(kind='bar')

plt.title("Average Likes per Platform")
plt.xlabel("Platform")
plt.ylabel("Average Likes")
plt.show()


#Cell 6 – Total Engagement (Likes + Shares + Comments)
data['total_engagement'] = data['likes'] + data['shares'] + data['comments']

data.groupby('platform')['total_engagement'].mean().plot(kind='bar')

plt.title("Average Total Engagement per Platform")
plt.xlabel("Platform")
plt.ylabel("Engagement")
plt.show()